AAA,EAAA;AAAA,SAAA;AACA,OALA;AAMA,KARA;AASA,IAAA,IATA,gBASA,OATA,EASA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,KAAA,IAAA,GAAA;AAAA,OAAA,EAAA,KAAA,CAAA,YAAA,CAAA,CAAA;AACA;AAXA,GAtBA;AAmCA,EAAA,KAAA,EAAA;AACA,IAAA,IAAA,EAAA,gBAAA;AACA,WAAA,QAAA,CAAA,KAAA,CAAA,IAAA,GAAA,KAAA,IAAA;AACA;AAHA;AAnCA,CAAA","sourcesContent":["<template>\r\n  <v-app>\r\n    <v-navigation-drawer\r\n      v-model=\"drawer\" app clipped>\r\n      <v-list dense>\r\n        <v-list-item-group color=\"primary\">\r\n          <v-list-item v-for=\"(menu, index) in menus\" :key=\"index\" @click=\"goto(menu.path)\">\r\n            <v-list-item-action>\r\n              <v-icon>{{ menu.icon }}</v-icon>\r\n            </v-list-item-action>\r\n            <v-list-item-content>\r\n              <v-list-item-title>{{ menu.title }}</v-list-item-title>\r\n            </v-list-item-content>\r\n          </v-list-item>\r\n        </v-list-item-group>\r\n      </v-list>\r\n    </v-navigation-drawer>\r\n\r\n    <v-app-bar app clipped-left dark color=\"primary\">\r\n      <v-app-bar-nav-icon @click.stop=\"drawer = !drawer\"></v-app-bar-nav-icon>\r\n      <v-toolbar-title>Application</v-toolbar-title>\r\n      <v-spacer />\r\n      <v-btn text icon @click=\"dark = !dark\">\r\n        <v-icon>{{ dark ? 'mdi-brightness-4' : 'mdi-brightness-6'}}</v-icon>\r\n      </v-btn>\r\n      <v-btn text icon @click=\"logOut()\"><v-icon>mdi-power</v-icon></v-btn>\r\n    </v-app-bar>\r\n\r\n    <v-main>\r\n      <v-container fluid>\r\n        <v-row>\r\n          <v-col>\r\n            <router-view />\r\n          </v-col>\r\n        </v-row>\r\n      </v-container>\r\n    </v-main>\r\n\r\n    <!-- <v-footer app>\r\n      <div class=\"flex text-center\">\r\n        <span class=\"lead text--secondary text-center\" >Software & Design ❤️\r\n          <a href=\"https://ahm3tcelik.github.io\" class=\"text-decoration-none\" target=\"_blank\">Ahmet ÇELİK</a>\r\n        </span>\r\n      </div>\r\n    </v-footer> -->\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\nimport firebase from 'firebase';\r\nimport { mapActions } from 'vuex';\r\n\r\nexport default {\r\n  data: () => ({\r\n    dark: false,\r\n    path: '/admin',\r\n    drawer: null,\r\n    menus: [\r\n      { path: '', title: 'Dashboard', icon: 'mdi-view-dashboard' },\r\n      { path: '/sounds', title: 'Sounds', icon: 'mdi-playlist-music'},\r\n      { path: '/categories', title: 'Categories', icon: 'mdi-folder-multiple' },\r\n    ]\r\n  }),\r\n  created() {\r\n    this.dark = this.$vuetify.theme.dark\r\n    this.loadSounds;\r\n    this.loadCategories;\r\n  },\r\n  computed: {\r\n    ...mapActions({\r\n        loadSounds: 'sounds/loadSounds',\r\n        loadCategories: 'categories/loadCategories',\r\n    }),\r\n  },\r\n  methods: {\r\n    logOut() {\r\n      firebase\r\n        .auth()\r\n        .signOut()\r\n        .then(() => {\r\n          this.$router.replace({path: '/'});\r\n        });\r\n    },\r\n    goto(newPath) {\r\n      this.$router.push({path: this.path + newPath}).catch(()=>{});\r\n    }\r\n  },\r\n  watch: {\r\n    dark: function() {\r\n      this.$vuetify.theme.dark = this.dark;\r\n    }\r\n  }\r\n}\r\n</script>"],"sourceRoot":"src/views/admin"}]}