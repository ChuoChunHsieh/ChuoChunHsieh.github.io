i></li>\n              <li></li>\n              <li></li>\n              <li></li>\n              <li></li>\n              <li></li>\n              <li></li> \n              <li></li>\n              <li></li>\n              <li></li> \n              <li></li> \n              <li></li>\n              <li></li>\n              <li></li> \n            </ul> \n          </div> \n        </div> \n      \n  </div>  \n</template>\n \n\n<script> \n\n \n\n\n\nexport default {\n  \n  data() {\n    return {\n      dialog: false,\n      dialog2: false,\n\n// windowWidt:800,\n// windowHeight:600,\n//       amplitude : 45, // Height of wave\n//       xspacing : 16, // Distance between each horizontal location\n//       dx  : \"\", // Value for incrementing x\n//       angle : 0, // Start angle\n//       w  : \"\", // Width of entire wave\n//       period  : 500, // How many pixels before the wave repeats\n//       yvalues : \"\", \n//       yvalues1: \"\", // Array to store yvalues \n\n//       state1: {\n//                 color: 64\n//               },\n//               state2: {\n//                 color: 180\n//               },\n    }\n  },\n  components: {\n    p5: VueP5\n  },\n  methods: {\n    draw1(sketch) {\n      this.state1.color += 3;\n      this.state1.color %= 255;\n      sketch.background(this.state1.color);\n    },\n    draw2(sketch) {\n      this.state2.color += 2;\n      this.state2.color %= 255;\n      sketch.background(this.state2.color);\n    },\n    keyPressed(keyCode) {\n      if (keyCode == 82) {\n        // 'r' key\n        this.reset();\n      }\n    },\n    reset() {\n      this.state1.color = 0;\n      this.state2.color = 0;\n    }, \n\n    \n\n  },\n  mounted() {\n    // this.go();\n     \n  }\n}\n</script>\n\n<style scoped>\n/* https://codepen.io/AnzilkhaN/pen/aXVOpE */\n\n/* https://codepen.io/Shruti-Ag/pen/ZEEQBqB */\n\n\n.ball-box {\n\twidth: 50%;\n\tmin-width: 820px;\n\theight: 95px;\n\tdisplay: flex;\n\tflex-direction: column;\n\tjustify-content: space-between;\n\tposition: absolute;\n\ttop: 50%;\n\tleft: 50%;\n\ttransform: translate(-50%, -50%);\n\n  -moz-transform:rotate(65deg);\n  -webkit-transform:rotate(65deg);\n  -o-transform:rotate(65deg);\n  -ms-transform:rotate(65deg);\n  transform:rotate(65deg);\n  \n}\nul {\n\tdisplay: flex;\n\talign-items: space-between;\n\tjustify-content: space-around;\n\tpadding: 0;\n\tmargin: 0;\n}\nli {\n\tposition: relative;\n\twidth: 20px;\n\theight: 20px;\n\tborder-radius: 50%;\n\tbackground-color: #fed7aa;\n\tlist-style-type: none;\n\n\topacity: 0;\n\t\n\tanimation: 2s ease-in-out infinite;\n\tanimation-name: upwards;\n}\n.red-ball li {\n\tbackground-color: #65a30d;\n\tanimation-name: downwards;\n}\nli:nth-child(2) {\n\tanimation-delay: .1s;\n}\nli:nth-child(3) {\n\tanimation-delay: .2s;\n}\nli:nth-child(4) {\n\tanimation-delay: .3s;\n}\nli:nth-child(5) {\n\tanimation-delay: .4s;\n}\nli:nth-child(6) {\n\tanimation-delay: .5s;\n}\nli:nth-child(7) {\n\tanimation-delay: .6s;\n}\nli:nth-child(8) {\n\tanimation-delay: .7s;\n}\nli:nth-child(9) {\n\tanimation-delay: .8s;\n}\nli:nth-child(10) {\n\tanimation-delay: .9s;\n}\nli:nth-child(11) {\n\tanimation-delay: 1s;\n}\nli:nth-child(12) {\n\tanimation-delay: 1.1s;\n}\nli:nth-child(13) {\n\tanimation-delay: 1.2s;\n}\nli:nth-child(14) {\n\tanimation-delay: 1.3s;\n}\nli:nth-child(15) {\n\tanimation-delay: 1.4s;\n}\n\nli:nth-child(16) {\n\tanimation-delay: 1.5s;\n}\nli:nth-child(17) {\n\tanimation-delay: 1.7s;\n}\nli:nth-child(18) {\n\tanimation-delay: 1.8s;\n}\n\nli:nth-child(19) {\n\tanimation-delay: 1.9s;\n}\nli:nth-child(20) {\n\tanimation-delay: 2.0s;\n}\nli:nth-child(21) {\n\tanimation-delay: 2.1s;\n}\n\nli:nth-child(22) {\n\tanimation-delay: 2.2s;\n}\nli:nth-child(23) {\n\tanimation-delay: 2.3s;\n}\nli:nth-child(24) {\n\tanimation-delay: 2.4s;\n}\n\n@keyframes upwards {\n\tfrom {\n\t\topacity: 1;\n\t\ttransform: translateY(0);\n\t\tz-index: 50;\n\t}\n\t50% {\n\t\ttransform: translateY(125px)  scale(1.2);\n\t}\n\tto {\n\t\top